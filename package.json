{
	"name": "vorth",
	"version": "0.0.1",
	"description": "client side routing and renderer for classic static MPA",
	"main": "index.mjs",
	"types": "./types/index.d.mts",
	"module": "index.mjs",
	"type": "module",
	"scripts": {
		"dev": "concurrently  \"bun --watch ./dev/index.mjs\" \"bun tsc --watch\" \"bun build --minify --watch ./index.mjs --outfile=./build/index.mjs --format=esm\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hakimjazuli/vorth.git"
	},
	"keywords": [
		"queue",
		"auto queue",
		"csr",
		"mpa",
		"static",
		"client side router",
		"client side routing",
		"attributeName"
	],
	"author": "hakim_jazuli",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/hakimjazuli/vorth/issues"
	},
	"homepage": "https://github.com/hakimjazuli/vorth#readme",
	"dependencies": {
		"concurrently": "^9.0.1",
		"virst": "^0.12.3"
	},
	"sideEffects": false,
	"devDependencies": {
		"@html_first/js_lib_template": "^1.3.11"
	}
}
